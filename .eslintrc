{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "semi": [2, "never"],
    "quotes": [2, "double"],
    "no-unused-vars": [2, {"varsIgnorePattern": "^React$"}]
  },
  "env": {
    "node": true,
    "browser": true,
    "phantomjs": true
  }
}
