{
  "name": "relei",
  "version": "0.0.2",
  "description": "Simple Relay client",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:milankinen/relei.git"
  },
  "keywords": [
    "react",
    "relay"
  ],
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run dist",
    "dist": "babel src --source-maps inline --out-dir lib",
    "test": "npm run lint && npm run smoke-tests",
    "smoke-tests": "babel-tape-runner test/smokeTests.js ; EXIT_VALUE=$? ; killall node ; exit $EXIT_VALUE",
    "lint": "eslint src test",
    "create-test-bundle": "cd test/server && browserify -t [ babelify --plugins './babel-ql' ] public/app.js > public/bundle.js",
    "start-test-server": "babel-node test/server/index.js"
  },
  "dependencies": {
    "react-relay": "^0.4.0"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "4.1.3",
    "babel-relay-plugin": "^0.3.0",
    "babel-tape-runner": "1.2.0",
    "babelify": "^6.4.0",
    "baconjs": "0.7.77",
    "bluebird": "^2.10.2",
    "browserify": "^11.2.0",
    "eslint": "1.7.3",
    "express": "^4.13.3",
    "express-graphql": "^0.4.0",
    "faucet": "0.0.1",
    "graphql": "^0.4.9",
    "graphql-relay": "^0.3.4",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-relay": "^0.4.0",
    "shelljs": "^0.5.3",
    "tape": "4.2.2",
    "zombie": "^4.1.0"
  }
}
